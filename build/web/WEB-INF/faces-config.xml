<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
   
    <managed-bean>
        <managed-bean-name>vendor</managed-bean-name>
        <managed-bean-class>DAO.bean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>id</property-name>
            <value>#{param.id}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ven</managed-bean-name>
        <managed-bean-class>DAO.VendorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>Conf</managed-bean-name>
        <managed-bean-class>DAO.Conf</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>navigate</managed-bean-name>
        <managed-bean-class>DAO.Navigate</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>usertest</managed-bean-name>
        <managed-bean-class>DAO.usertest</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>accessuser</managed-bean-name>
        <managed-bean-class>DAO.accessuser</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>newclass</managed-bean-name>
        <managed-bean-class>DAO.newclass</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/welcomeJSF.jsp</from-view-id>
        <navigation-case>
            <from-outcome>yes</from-outcome>
            <to-view-id>/AdminLogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/citizenlogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>roadshigh</from-outcome>
            <to-view-id>/roadshighways.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>immigration</from-outcome>
            <to-view-id>/immigrationoffice.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>union</from-outcome>
            <to-view-id>/upndcity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>police</from-outcome>
            <to-view-id>/policedept.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>passport</from-outcome>
            <to-view-id>/passportoffice.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ha</from-outcome>
            <to-view-id>/Registration_Citizen.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Registration_Citizen.jsp</from-view-id>
        <navigation-case>
            <from-outcome>check</from-outcome>
            <to-view-id>/checkinformation.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>RegistrationShow</from-outcome>
            <to-view-id>/RegistrationShow.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/AdminLogin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>accesscontroll</from-outcome>
            <to-view-id>/Access.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>citizeninformation</from-outcome>
            <to-view-id>/try.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/citizenlogin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/newjsf.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/try.jsp</from-view-id>
        <navigation-case>
            <from-outcome>newinf</from-outcome>
            <to-view-id>/newinf.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
